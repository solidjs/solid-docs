{
	"compilerOptions": {
		"skipLibCheck": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "node",
		"jsxImportSource": "solid-js",
		"jsx": "preserve",
		"strict": true,
		"baseUrl": "./",
		"paths": {
			"~/*": ["./src/*"]
		}
	},
	"exclude": ["node_modules"]
}
